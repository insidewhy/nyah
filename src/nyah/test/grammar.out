file grammar.nyah
grammar "grammar" = [
    Rule: (
        RuleName: "Spacing"
        '-'
        Expression: Suffix: (
            Expression: OrderedChoice: [
                "\s"
                Joined: [
                    "//"
                    Suffix: (
                        Expression: Prefix: (
                            '!'
                            "\n"
                        )
                        '*'
                    )
                    "\n"
                ]
            ]
            '+'
        )
    )
    Rule: (
        RuleName: "AnyCharacter"
        '-'
        Expression: "."
    )
    Rule: (
        RuleName: "CharacterRange"
        '='
        Expression: Joined: [
            "["
            Suffix: (
                Expression: OrderedChoice: [
                    Joined: [
                        CharacterRange: [ "\\" ]
                        CharacterRange: [
                            "\\"
                            "\]"
                            's'
                            'S'
                            'n'
                            'N'
                            't'
                            'T'
                            'w'
                            'W'
                        ]
                    ]
                    Expression: Joined: [
                        '.'
                        "-"
                        '.'
                    ]
                    '.'
                ]
                '*'
            )
            "]"
        ]
    )
    Rule: (
        RuleName: "Escape"
        '-'
        Expression: Joined: [
            CharacterRange: [ "\\" ]
            CharacterRange: [
                's'
                'S'
                'n'
                'N'
                't'
                'T'
                'w'
                'W'
                '.'
                '"'
                '\''
                '&'
                '!'
                '+'
                '*'
                "\\"
            ]
        ]
    )
    Rule: (
        RuleName: "String"
        '-'
        Expression: OrderedChoice: [
            Joined: [
                "\""
                Suffix: (
                    Expression: OrderedChoice: [
                        RuleName: "Escape"
                        Prefix: (
                            '!'
                            "\""
                        )
                    ]
                    "^*"
                )
                "\""
            ]
            Joined: [
                "'"
                Suffix: (
                    Expression: OrderedChoice: [
                        RuleName: "Escape"
                        Prefix: (
                            '!'
                            "'"
                        )
                    ]
                    "^*"
                )
                "'"
            ]
        ]
    )
    Rule: (
        RuleName: "RuleIdentifier"
        '-'
        Expression: Joined: [
            CharacterRange: [ (
                'A'
                'Z'
            ) ]
            Suffix: (
                CharacterRange: [
                    (
                        'a'
                        'z'
                    )
                    (
                        'A'
                        'Z'
                    )
                    (
                        '0'
                        '9'
                    )
                    '_'
                ]
                '+'
            )
        ]
    )
    Rule: (
        RuleName: "GrammarIdentifier"
        '-'
        Expression: Joined: [
            CharacterRange: [
                (
                    'a'
                    'z'
                )
                (
                    'A'
                    'Z'
                )
            ]
            Suffix: (
                CharacterRange: [
                    (
                        'a'
                        'z'
                    )
                    (
                        'A'
                        'Z'
                    )
                    (
                        '0'
                        '9'
                    )
                    '_'
                ]
                '+'
            )
        ]
    )
    Rule: (
        RuleName: "RuleName"
        '='
        Expression: RuleName: "RuleIdentifier"
    )
    Rule: (
        RuleName: "ScopedRule"
        '='
        Expression: Sequence: [
            RuleName: "GrammarIdentifier"
            ":"
            RuleName: "RuleIdentifier"
        ]
    )
    Rule: (
        RuleName: "Primary"
        '-'
        Expression: OrderedChoice: [
            RuleName: "String"
            RuleName: "CharacterRange"
            RuleName: "Escape"
            RuleName: "AnyCharacter"
            RuleName: "ScopedRule"
            Sequence: [
                RuleName: "RuleName"
                Prefix: (
                    "&!"
                    "<"
                )
            ]
            Sequence: [
                "("
                RuleName: "Expression"
                ")"
            ]
        ]
    )
    Rule: (
        RuleName: "Suffix"
        '='
        Expression: Sequence: [
            RuleName: "Primary"
            Suffix: (
                Expression: OrderedChoice: [
                    "^+"
                    "^*"
                    CharacterRange: [
                        '?'
                        '*'
                        '+'
                    ]
                    "|+"
                    "|?"
                ]
                "|?"
            )
        ]
    )
    Rule: (
        RuleName: "Prefix"
        '='
        Expression: Sequence: [
            Suffix: (
                Expression: OrderedChoice: [
                    "&!"
                    CharacterRange: [
                        '&'
                        '!'
                    ]
                ]
                "|?"
            )
            RuleName: "Suffix"
        ]
    )
    Rule: (
        RuleName: "Join"
        '='
        Expression: Sequence: [
            RuleName: "Prefix"
            Expression: OrderedChoice: [
                "^%"
                "%+"
                "%"
                "|%"
            ]
            RuleName: "Prefix"
        ]
    )
    Rule: (
        RuleName: "Joined"
        '='
        Expression: Join: (
            Expression: OrderedChoice: [
                RuleName: "Join"
                RuleName: "Prefix"
            ]
            "|%"
            "^"
        )
    )
    Rule: (
        RuleName: "Sequence"
        '='
        Expression: Suffix: (
            RuleName: "Joined"
            "|+"
        )
    )
    Rule: (
        RuleName: "OrderedChoice"
        '='
        Expression: Join: (
            RuleName: "Sequence"
            "|%"
            "/"
        )
    )
    Rule: (
        RuleName: "Expression"
        '='
        Expression: RuleName: "OrderedChoice"
    )
    Rule: (
        RuleName: "Rule"
        '='
        Expression: Sequence: [
            RuleName: "RuleName"
            "<"
            CharacterRange: [
                '-'
                '='
            ]
            RuleName: "Expression"
        ]
    )
    Rule: (
        RuleName: "Grammar"
        '-'
        Expression: Suffix: (
            RuleName: "Rule"
            '+'
        )
    )
]
