file nyah.nyah
grammar "nyah" extends "grammar" = [
    "Grammar": Rule: (
        '-'
        Expression: Suffix: (
            "MetaGrammar"
            '+'
        )
    )
    "MetaIdentifier": Rule: (
        '-'
        Expression: Joined: [
            CharacterRange: [
                (
                    'A'
                    'Z'
                )
                (
                    'a'
                    'z'
                )
                '_'
            ]
            Suffix: (
                CharacterRange: [
                    (
                        'A'
                        'Z'
                    )
                    (
                        'a'
                        'z'
                    )
                    (
                        '0'
                        '9'
                    )
                    '_'
                ]
                '*'
            )
        ]
    )
    "MetaGrammar": Rule: (
        '='
        Expression: Sequence: [
            String: "@grammar"
            "MetaIdentifier"
            Suffix: (
                Expression: Sequence: [
                    String: "@extends"
                    "MetaIdentifier"
                ]
                '?'
            )
            ScopedRule: (
                "grammar"
                "Grammar"
            )
        ]
    )
]
