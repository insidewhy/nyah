private idFirst := 'a' .. 'z' | 'A' .. 'Z' | '_' ;

id := idFirst (idFirst | \d) * ;
character := "'" ^ < ! ("\n" | "'") > ^ "'" | '"' ^ < ! ("\n" | "'") > ^ '"';

quotedString := "'" ^ < (! ("\n" | "'") )* > ^ "'" | '"' ^ < (! ( "\n" | '"')* ) > ^ '"';

class CharacterRange {
    character start;
    character end;
    begin := start '..' end
}

